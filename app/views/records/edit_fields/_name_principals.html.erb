<% if not @generic_file.nil? %>
<% if defined? props %>
<% @clabel = props[:label]  %>
<% else %>
<% @clabel = nil? %>
<% end %>

<%= f.label :name_principals, label: @clabel %>
<% (@generic_file.name_principals + [MODS::MADS::Name.new]).each do |name| %>
	<%= f.simple_fields_for :name_principals, name do |builder| %>
		<%= render_edit_field_partial(:label, {f: builder, label_text: false}) %>
	<% end %>
<% end %>

<% end %>


