<% if not @generic_file.nil? %>
<% if defined? props %>
<% @clabel = props[:label]  %>
<% else %>
<% @clabel = nil? %>
<% end %>

<%= f.label :subject_genres, label: @clabel %>
<% (@generic_file.subject_genres + [MODS::MADS::GenreForm.new]).each do |subject| %>
	<%= f.simple_fields_for :subject_genres, subject do |builder| %>
		<%= render_edit_field_partial(:label, {f: builder, label_text: false}) %>
	<% end %>
<% end %>

<% end %>


