<% if not @generic_file.nil? %>
<%= f.label :subject_occupations %>
<% (@generic_file.subject_occupations + [MODS::MADS::Occupation.new]).each do |subject| %>
	<%= f.simple_fields_for :subject_occupations, subject do |builder| %>
		<%= render_edit_field_partial(:label, {f: builder, label_text: false}) %>
	<% end %>
<% end %>

<% end %>


