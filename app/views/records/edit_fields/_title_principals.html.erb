<% if not @generic_file.nil? %>
<% if defined? props %>
<% @clabel = props[:label]  %>
<% else %>
<% @clabel = nil? %>
<% end %>

<%= f.input :title_principals, label: @clabel, as: :nested_attribute_multiple, :fields => [:label, :abbreviated_variant, :translated_variant, :alternative_variant], :multiple => false %>

<% end %>



