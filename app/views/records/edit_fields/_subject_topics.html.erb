<% if not @generic_file.nil? %>
<%= f.label :subject_topics %>
<% (@generic_file.subject_topics + [MODS::MADS::Topic.new]).each do |subject| %>
	<%= f.simple_fields_for :subject_topics, subject do |builder| %>
		<%= render_edit_field_partial(:label, {f: builder, label_text: false}) %>
	<% end %>
<% end %>

<% end %>


